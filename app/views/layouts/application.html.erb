<!DOCTYPE html>
<html>
<head>
  <title>Stack</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag    "patternfly.css", "patternfly" %>
  <%= stylesheet_link_tag    "patternfly.min.css", "patternfly" %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>
  <%= javascript_include_tag "https://api.stackexchange.com/js/2.0/all.js" %>
  <%= javascript_include_tag "jquery.scrollNav.min.js" %>
  <%= csrf_meta_tags %>
</head>
	<body>
		<%= render 'layouts/header' %>
		<div class="container">
			<% flash.each do |key, value| %>
			<div class="alert alert-<%= key %>">
				<%= value %>
			</div>
			<% end %>
			<%= yield %>
			<%= render 'layouts/footer'%>
			<%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>
	
